{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "AllowedHosts": "*",
  "AntiBruteforceOptions": {
    "Concurrent": 3,
    "Timeout": "00:00:10"
  },
  "QueryStringAuthenticationOptions": {
    "QueryStringParameterName": "token"
  }
}
